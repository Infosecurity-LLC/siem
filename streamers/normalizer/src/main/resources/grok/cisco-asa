# Cisco ASA
# = common =
CISCO_TIMESTAMP %{MONTH:month} +%{MONTHDAY:day}(?: %{YEAR:year})? %{TIME:time}( %{TZ:timezone})?
CISCO_BEGIN ( )?(<%{INT}>)?(%{INT})?(:)?( )?
CISCO_MAYBE_IP ( %{IP})?( )?(:)?
CISCO_SOURCE_IMP_ID %%{WORD:eventSourceSubsys}(-auth|-sys|-config|-session|-vpnc)?-%{INT:interactionImportance}
CISCO_HEAD %{CISCO_BEGIN}%{CISCO_TIMESTAMP:originTime}%{CISCO_MAYBE_IP} (:)?%{CISCO_SOURCE_IMP_ID}-%{INT:datapayloadMsgId}
CISCO_USER_OR_STARS %{WORD}|\*{5}
CISCO_USER user = %{CISCO_USER_OR_STARS:subjectName}
CISCO_WORD \b[\w\-]+\b
# Extract message ID
CISCOASA_ID %(%{WORD})(-auth|-sys|-config|-session|-vpnc)?-%{INT}-%{INT:id}
# ASA-113004
CISCOASA_113004 %{CISCO_HEAD}: AAA user (authentication|accounting) Successful : server =  %{IPORHOST:aux1} : %{CISCO_USER}
# ASA-113012
CISCOASA_113012 %{CISCO_HEAD}: AAA user authentication Successful : local database : %{CISCO_USER}
# ASA-113015
CISCOASA_113015 %{CISCO_HEAD}: AAA user authentication Rejected : reason = %{GREEDYDATA:interactionReason} : local database : %{CISCO_USER}( : user IP = %{IP:sourceLocationIp})?
# ASA-605005
